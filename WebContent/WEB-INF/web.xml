<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>facebookla</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Registers Users</description>
    <display-name>Reg</display-name>
    <servlet-name>Reg</servlet-name>
    <servlet-class>com.tahir.facebookla.controllers.Reg</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Reg</servlet-name>
    <url-pattern>/reg</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginController</display-name>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>com.tahir.facebookla.controllers.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AdminViewUsers</display-name>
    <servlet-name>AdminViewUser</servlet-name>
    <servlet-class>com.tahir.facebookla.controllers.AdminViewUsers</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminViewUser</servlet-name>
    <url-pattern>/admin/viewusers</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AdminDeleteUser</display-name>
    <servlet-name>AdminDeleteUser</servlet-name>
    <servlet-class>com.tahir.facebookla.controllers.AdminDeleteUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminDeleteUser</servlet-name>
    <url-pattern>/admin/deleteuser</url-pattern>
  </servlet-mapping>
</web-app>